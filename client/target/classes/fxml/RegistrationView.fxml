<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.AnchorPane?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" prefHeight="196.0" prefWidth="407.0"
            xmlns="http://javafx.com/javafx"
            fx:controller="de.uol.swp.client.register.RegistrationPresenter" stylesheets="@../css/myDialog.css" styleClass="dialog-pane">
    <Label layoutX="43.0" layoutY="17.0" text="Login"/>
    <TextField id="loginField" fx:id="loginField" layoutX="142.0" layoutY="12.0" promptText="Enter Login">
        <tooltip>
            <Tooltip text="Enter your login here"/>
        </tooltip>
    </TextField>
    <PasswordField id="passwordField" fx:id="passwordField1" layoutX="142.0" layoutY="48.0" promptText="Enter Password"/>
    <PasswordField id="passwordField" fx:id="passwordField2" layoutX="142.0" layoutY="85.0"
                   promptText="Repeat Password"/>
    <Button id="registerButton" defaultButton="true"  layoutX="112.0" layoutY="133.0" mnemonicParsing="false"
            onAction="#onRegisterButtonPressed" text="Register"/>
    <Button id="cancelButton" layoutX="203.0" layoutY="133.0"
            mnemonicParsing="false" onAction="#onCancelButtonPressed" text="Cancel"/>

    <Label layoutX="44.0" layoutY="53.0" text="Password"/>
    <Label layoutX="44.0" layoutY="90.0" text="Password"/>
</AnchorPane>
