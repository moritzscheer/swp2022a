<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.Tooltip?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>

<GridPane id="Background" maxHeight="1080.0" maxWidth="1920.0" minHeight="600.0" minWidth="1000.0" prefHeight="600.0"
          prefWidth="1000.0" stylesheets="@../css/BaseViewStyle.css" xmlns="http://javafx.com/javafx/17.0.2-ea"
          xmlns:fx="http://javafx.com/fxml/1" fx:controller="de.uol.swp.client.register.RegistrationPresenter">
    <columnConstraints>
        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
        <ColumnConstraints hgrow="SOMETIMES" maxWidth="329.0" minWidth="10.0" prefWidth="246.0"/>
        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
    </columnConstraints>
    <rowConstraints>
        <RowConstraints maxHeight="336.0" minHeight="10.0" prefHeight="78.0" vgrow="SOMETIMES"/>
        <RowConstraints maxHeight="336.0" minHeight="10.0" prefHeight="212.0" vgrow="SOMETIMES"/>
        <RowConstraints maxHeight="362.0" minHeight="10.0" prefHeight="105.0" vgrow="SOMETIMES"/>
        <RowConstraints maxHeight="430.0" minHeight="10.0" prefHeight="145.0" vgrow="SOMETIMES"/>
        <RowConstraints maxHeight="303.0" minHeight="10.0" prefHeight="76.0" vgrow="SOMETIMES"/>
        <RowConstraints maxHeight="241.0" minHeight="10.0" prefHeight="85.0" vgrow="SOMETIMES"/>
    </rowConstraints>
    <GridPane id="LogoBox" stylesheets="@../css/BaseViewStyle.css" GridPane.columnIndex="1" GridPane.columnSpan="5"
              GridPane.rowIndex="1">
      <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
      </columnConstraints>
      <rowConstraints>
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
      </rowConstraints>
    </GridPane>
    <GridPane alignment="CENTER" prefHeight="150.0" prefWidth="300.0" scaleShape="false" styleClass="InnerBox"
              GridPane.columnIndex="3" GridPane.rowIndex="3">
        <columnConstraints>
            <ColumnConstraints fillWidth="false" halignment="CENTER" hgrow="NEVER" maxWidth="-Infinity"
                               minWidth="10.0" prefWidth="150.0"/>
            <ColumnConstraints fillWidth="false" halignment="CENTER" hgrow="NEVER" maxWidth="-Infinity"
                               minWidth="10.0" prefWidth="150.0"/>
        </columnConstraints>
        <rowConstraints>
            <RowConstraints fillHeight="false" maxHeight="-Infinity" minHeight="10.0" prefHeight="30.0"
                            valignment="CENTER" vgrow="NEVER"/>
            <RowConstraints fillHeight="false" maxHeight="-Infinity" minHeight="10.0" prefHeight="30.0"
                            valignment="CENTER" vgrow="NEVER"/>
            <RowConstraints fillHeight="false" maxHeight="-Infinity" minHeight="10.0" prefHeight="30.0"
                            valignment="CENTER" vgrow="NEVER"/>
            <RowConstraints fillHeight="false" maxHeight="-Infinity" minHeight="10.0" prefHeight="30.0"
                            valignment="CENTER" vgrow="NEVER"/>
        </rowConstraints>
        <opaqueInsets>
            <Insets/>
        </opaqueInsets>
        <Label id="nameLogin" prefHeight="29.0" prefWidth="35.0" text="Login:" GridPane.halignment="CENTER"
               GridPane.valignment="CENTER">
            <GridPane.margin>
                <Insets top="5.0"/>
            </GridPane.margin>
        </Label>
        <TextField id="loginField" fx:id="loginField" promptText="Your name" GridPane.columnIndex="1">
            <tooltip>
                <Tooltip text="Enter your name here"/>
            </tooltip>
            <GridPane.margin>
                <Insets right="10.0" top="5.0"/>
            </GridPane.margin>
        </TextField>
        <PasswordField id="passwordField" fx:id="passwordField1" promptText="Enter password" GridPane.columnIndex="1"
                       GridPane.rowIndex="1">
            <GridPane.margin>
                <Insets right="10.0" top="5.0"/>
            </GridPane.margin>
        </PasswordField>
        <PasswordField id="passwordField" fx:id="passwordField2" promptText="Repeat password"
                       GridPane.columnIndex="1" GridPane.rowIndex="2">
            <GridPane.margin>
                <Insets right="10.0" top="5.0"/>
            </GridPane.margin>
        </PasswordField>
        <Button id="registerButton" defaultButton="true" mnemonicParsing="false" onAction="#onRegisterButtonPressed"
                text="Register" GridPane.columnIndex="1" GridPane.hgrow="NEVER" GridPane.rowIndex="3"
                GridPane.vgrow="NEVER"/>
        <Button id="cancelButton" mnemonicParsing="false" onAction="#onCancelButtonPressed" text="Cancel"
                GridPane.hgrow="NEVER" GridPane.rowIndex="3" GridPane.vgrow="NEVER"/>
        <Label id="passwordLogin" prefHeight="29.0" prefWidth="58.0" text="Password:" GridPane.rowIndex="1">
            <GridPane.margin>
                <Insets top="5.0"/>
            </GridPane.margin>
        </Label>
        <Label id="passwordLogin" prefHeight="29.0" prefWidth="56.0" text="Password:" GridPane.rowIndex="2">
            <GridPane.margin>
                <Insets top="5.0"/>
            </GridPane.margin>
        </Label>
    </GridPane>
    <Button id="registerButton" minWidth="60.0" mnemonicParsing="false" onAction="#onRegisterButtonPressed"
            prefHeight="41.0" prefWidth="83.0" stylesheets="@../css/BaseViewStyle.css" text="Exit"
            GridPane.columnIndex="5" GridPane.halignment="CENTER" GridPane.rowIndex="4" GridPane.valignment="CENTER"/>
</GridPane>
